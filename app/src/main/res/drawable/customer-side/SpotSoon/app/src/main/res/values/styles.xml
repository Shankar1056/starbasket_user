    <style name="CustomThemeApp" parent="android:Theme.Holo.Light.DarkActionBar">
        <item name="android:actionBarStyle">@style/MyActionBarregister</item>
    </style>
    <style name="MyActionBarregister" paren="android:style/Widget.Holo.Light.ActionBar.Solid.Inverse">

        <item name="android:height">?android:actionBarSize</item>
        <item name="android:text">25dp</item>
    </style>
    <style name="CustomThemeAppsign" parent="android:Theme.Holo.Light.DarkActionBar">
        <item name="android:actionBarStyle">@style/MyActionBarsign</item>
    </style>
    <style name="MyActionBarsign" paren="android:style/Widget.Holo.Light.ActionBar.Solid.Inverse">

