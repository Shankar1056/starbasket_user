    </style>

    <style name="CustomThemeAppcloseopn" parent="android:Theme.Holo.Light.DarkActionBar">
        <item name="android:actionBarStyle">@style/MyActionbarcloseopn</item>
    </style>
    <style name="MyActionbarcloseopn" paren="android:style/Widget.Holo.Light.ActionBar.Solid.Inverse">

        <item name="android:height">96dp</item>
        <item name="android:text">18dp</item>
    </style>

